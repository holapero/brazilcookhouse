<h1>Sign Up</h1>
  <%= form_for(@users) do |f| %>
    <fieldset>
      <section>
        <%= f.label :name %>
        <%= f.text_field (:name)%>
      </section>
      <section>
        <%= f.label :email %>
        <%= f.email_field (:email) %>
      </section>
      <section>
        <%= f.label :password %>
        <%= f.password_field (:password)%>
      </section>
      <section>
        <%= f.label :password_confirmation %>
        <%= f.password_field (:password_confimation)%>
      </section>
      <section>
        <%= f.submit("Sign Up") %>
      </section>
    </fieldset>
  <% end %>